<p id="notice"><%= notice %></p>

<h1>Company Aprovals</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Admin</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @company_aprovals.each do |company_aproval| %>
      <tr>
        <td><%= company_aproval.name %></td>
        <td><%= company_aproval.admin_id %></td>
        <td><%= company_aproval.note %></td>
        <td><%= link_to 'Show', company_aproval %></td>
        <td><%= link_to 'Edit', edit_company_aproval_path(company_aproval) %></td>
        <td><%= link_to 'Destroy', company_aproval, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Company Aproval', new_company_aproval_path %>
